<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_alignWithParentIfMissing="false"
    android:orientation="vertical"
    android:weightSum="1">
    <com.jetradar.desertplaceholder.DesertPlaceholder
        android:id="@+id/placeholder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:dp_buttonText="Add the subjects"
        app:dp_message="Nothing To Show\nFirst add the subjects"/>


    <TextView
        android:id="@+id/textView58"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:text="Your Attendance History :"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="16sp" />

    <ListView

        android:id="@+id/listView777"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView58"
        android:layout_marginTop="7dp"
        android:background="@color/cardview_shadow_end_color"
        android:cacheColorHint="@android:color/background_light"
        android:paddingLeft="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111"
        tools:layout_editor_absoluteX="8dp" />


</RelativeLayout>
